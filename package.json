{
  "name": "prisma-react",
  "version": "1.0.0",
  "description": "backend api data getter",
  "engines": {
    "node": "13.6.0",
    "npm": "6.13.4"
  },
  "main": "app.js",
  "scripts": {
    "client": "cd Application && npm start",
    "server": "nodemon app.js",
    "build": "cd Application && npm run build",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "start": "node app.js",
    "heroku-postbuild": "cd Application && npm install && npm install --only=dev --no-shrinkwrap && npm run build"
  },
  "dependencies": {
    "body-parser": "^1.20.1",
    "clarifai": "^2.9.1",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "dotenv": "7.0.0",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "concurrently": "^4.0.1",
    "nodemon": "^2.0.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/romanplkh/prisma-react"
  },
  "author": "Roman Pelikh",
  "license": "ISC"
}
